        "stop_on_stage_fail": false
    },

    "stages": {
        "dockertest": {
            "image_name": "dcanumn/docker-test",
            "container_args": {
                "-t": true
            },
            "action":"run",
            "mounts": [
                {
                    "host_path": "C:\\Users\\tikal004\\Desktop\\DCAN",
                    "container_path":"/test"
                }
            ],
            "positional_args": [
                "/test"
            ],
            "flags": {
                "-l": true
            }
        },
        "dockerfail": {
            "image_name":"dcanumn/dockerfail:1.4",
            "container_args": {},
            "action": "run",
            "mounts": [],
            "positional_args": [],
            "flags":{}
        }
    }
}
